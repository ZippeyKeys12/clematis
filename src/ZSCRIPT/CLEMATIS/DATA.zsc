class Cl_Result{
    bool Success;

    Name Name;

    ZE_ErrorSeverity Severity;

    static
    Cl_Result Create(Name Name, bool Success, ZE_ErrorSeverity Severity){
        Cl_Result Result=new('Cl_Result');
        Result.Name=Name;
        Result.Success=Success;
        Result.Severity=Severity;
        return Result;
    }
}